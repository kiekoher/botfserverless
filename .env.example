# Example environment variables
# Copy this file to .env and fill in your values

# Gemini API key for embedding and generation
GEMINI_API_KEY=

# Supabase connection settings
SUPABASE_URL=
SUPABASE_KEY=
SUPABASE_SERVICE_KEY=

# Ingestion settings
# Optional: directory to watch for new documents (default /app/documentos)
INGEST_DIR=
# Optional: polling interval in milliseconds (default 5000)
INGEST_INTERVAL_MS=

